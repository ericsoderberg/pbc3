<%= label_tag :Choices %>
  
<table id="form_field_<%= @form_field.id %>_options">
  <tr><th>Label</th><th>Type</th><th>Size</th><th>Help</th><th></th></tr>
  <%= render :partial => 'form_field_options/show_option',
    :collection => @form_field.form_field_options %>
</table>

<div class="add_content">
  <%= link_to 'Add option',
    new_form_field_option_path(@form, @form_field), :remote => true %>
</div>
